 if (w < 0) w = 0;
 if ((x + w) > (dispWin.x2+1)) w = dispWin.x2 - x + 1;
 if (w == 0) w = 1;

 TFT_pushColorRep(x, y, x+w-1, y, color, (uint32_t)w);
